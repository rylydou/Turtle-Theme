{
	"name": "turtle-theme",
	"displayName": "Turtle Theme",
	"version": "2.0.0",
	"description": "A theme by a turtle, for turtles",
	"publisher": "CiberTurtle",
	"icon": "images/icon.png",
	"license": "MIT",
	"galleryBanner": {
		"color": "#FF7504",
		"theme": "dark"
	},
	"categories": [
		"Themes"
	],
	"engines": {
		"vscode": "^1.63.0"
	},
	"bugs": {
		"url": "https://github.com/CiberTurtle/Turtle-Theme/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CiberTurtle/Turtle-Theme"
	},
	"contributes": {
		"themes": [
			{
				"id": "zuhgy-dark",
				"label": "Turtle Theme - Zuhgy Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/zuhgy-dark.json"
			},
			{
				"id": "zuhgy-light",
				"label": "Turtle Theme - Zuhgy Light",
				"uiTheme": "vs",
				"path": "./themes/zuhgy-light.json"
			},
			{
				"id": "classic-dark",
				"label": "Turtle Theme - Classic Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/classic-dark.json"
			}
		]
	},
	"scripts": {
		"package": "mkdir -p bin && vsce package -o bin",
		"release": "vsce publish"
	},
	"devDependencies": {
		"@vscode/vsce": "^2.22.0"
	}
}
