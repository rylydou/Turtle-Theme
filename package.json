{
	"name": "turtle-theme",
	"displayName": "Turtle Theme Collection",
	"version": "2.0.0",
	"description": "A collection of themes by a turtle, for turtles",
	"publisher": "CiberTurtle",
	"icon": "images/icon.png",
	"license": "MIT",
	"bugs": "https://github.com/CiberTurtle/Turtle-Theme/issues",
	"repository": "https://github.com/CiberTurtle/Turtle-Theme",
	"galleryBanner": {
		"color": "#FF7504",
		"theme": "dark"
	},
	"categories": [
		"Themes"
	],
	"engines": {
		"vscode": "^1.63.0"
	},
	"contributes": {
		"themes": [
			{
				"id": "zuhgy-dark",
				"label": "Turtle Theme - Zuhgy Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/zuhgy-dark.json"
			},
			{
				"id": "zuhgy-light",
				"label": "Turtle Theme - Zuhgy Light",
				"uiTheme": "vs",
				"path": "./themes/zuhgy-light.json"
			},
			{
				"id": "classic-dark",
				"label": "Turtle Theme - Classic Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/classic-dark.json"
			}
		]
	},
	"scripts": {
		"pack": "mkdir -p dist && vsce pack -o dist",
		"release": "vsce publish",
		"dogfood": "vsce pack -o dist/dogfood.vsix && code --install-extension dist/dogfood.vsix"
	},
	"devDependencies": {
		"@vscode/vsce": "^2.22.0"
	}
}
